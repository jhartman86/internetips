{
  "name": "internetips",
  "version": "0.1.0",
  "description": "A tooltip library without the whole kitchen sink",
  "main": "dist/node/index.js",
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "build": "babel lib/ -d dist/node/ && webpack && node-sass --output ./dist/ ./lib/style.scss && npm run-script copy-build-assets",
    "copy-build-assets": "cp ./dist/bundled.min.js ./docs/ && cp ./dist/style.css ./docs/",
    "lint": "eslint lib/ test/",
    "start": "webpack-dev-server --open",
    "test": "karma start ./test/karma.conf.js --single-run",
    "test-watch": "karma start ./test/karma.conf.js --log-level debug",
    "prepublish": "npm run build",
    "prebuild": "npm run test",
    "pretest": "npm run lint",
    "preversion": "npm run build",
    "version": "git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "author": "Jon Hartman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jhartman86/internetips.git"
  },
  "keywords": [
    "tooltips"
  ],
  "bugs": {
    "url": "https://github.com/jhartman86/internetips/issues"
  },
  "homepage": "https://github.com/jhartman86/internetips#readme",
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-loader": "6.4.1",
    "babel-preset-es2015": "6.24.0",
    "chai": "3.5.0",
    "eslint": "3.18.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.3",
    "karma-webpack": "2.0.3",
    "lodash": "4.17.4",
    "mocha": "3.2.0",
    "node-sass": "4.5.1",
    "sinon": "2.1.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.2"
  }
}
